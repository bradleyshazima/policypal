{
  "expo": {
    "name": "Policy Pal",
    "slug": "insurance-reminder-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/applogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fefefe"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uglabs.policypal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/applogo.png",
        "backgroundColor": "#fefefe"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.uglabs.policypal"
    },
    "web": {
      "favicon": "./assets/applogo.png"
    },
    "plugins": [
      "expo-mail-composer"
    ]
  }
}
